{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth != null",
        ".write": "$uid === auth.uid"
      }
    },
    "priceCache": {
      "$coin": {
        ".read": true,
        ".write": true
      }
    },
    "priceTokenHyper": {
      ".read": true,
      "$coin": {
        ".read": true,
        ".write": true
      }
    },
    "priceTokenBinance": {
      ".read": true,
      "$coin": {
        ".read": true,
        ".write": true
      }
    }
  }
}
